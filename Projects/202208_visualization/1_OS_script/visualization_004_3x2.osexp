set mouse_backend psycho
set keyboard_backend psycho
set height 768
set fullscreen no
set form_clicks no
set foreground white
set font_underline no
set font_size 18
set font_italic no
set font_family mono
set font_bold no
set experiment_path "D:\\core\\Research\\projects\\Meta_Replication\\Projects\\202208_visualization\\1_OS_script"
