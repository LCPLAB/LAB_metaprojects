define sketchpad EXP_Instruction_sc
	set duration keypress
	set description "Displays stimuli"
	draw textline center=1 color=white font_bold=no font_family=mono font_italic=no font_size=28 html=yes show_if=always text="练习结束，即将进入正式实验<br/><br/>请记得：<b>每题开始字词出现时，请您想像字词形容的图像</b><br/><b>图像出现时请立即反应是否符合形容</b><br/><br/><br/>请您保持专注，完成实验<br/><br/><br/>按任意键进入正式实验" x=0 y=90 z_index=0

define sketchpad EXP_Instruction_tc
	set duration keypress
	set description "Displays stimuli"
	draw textline center=1 color=white font_bold=no font_family=mono font_italic=no font_size=28 html=yes show_if=always text="練習結束，即將進入正式實驗<br/><br/>請記得：<b>每題開始字詞出現時，請您想像字詞形容的圖像</b><br/><b>圖像出現時請立即反應是否符合形容</b><br/><br/><br/>請您保持專注，依指示完成實驗<br/><br/><br/>按任意鍵進入正式實驗" x=0 y=90 z_index=0

define sketchpad EXP_complete_sc
	set duration keypress
	set description "Displays stimuli"
	draw textline center=1 color=white font_bold=no font_family=mono font_italic=no font_size=24 html=yes show_if=always text="没有实验题目了<br/><br/>结束前请回答您的性别及出生年份资讯<br/><br/><br/><br/>请按任意键继续" x=0 y=-64 z_index=0

define sketchpad EXP_complete_tc
	set duration keypress
	set description "Displays stimuli"
	draw textline center=1 color=white font_bold=no font_family=mono font_italic=no font_size=24 html=yes show_if=always text="沒有實驗題目了<br/><br/>結束前請告知我們您的性別及出生年份資訊<br/><br/><br/><br/>請按任意鍵繼續" x=0 y=-64 z_index=0

define sketchpad EXP_end_sc
	set duration keypress
	set description "Displays stimuli"
	draw textline center=1 color=white font_bold=no font_family=mono font_italic=no font_size=36 html=yes show_if=always text="实验结束<br/><br/>请记下你的实验完成码<br/>[identifier]<br/><br/>按任意键关闭网页" x=0 y=-32 z_index=0

define sketchpad EXP_end_tc
	set duration keypress
	set description "Displays stimuli"
	draw textline center=1 color=white font_bold=no font_family=mono font_italic=no font_size=36 html=yes show_if=always text="實驗結束<br/><br/>請記下您的實驗完成碼<br/>[identifier]<br/>請將此完成碼與個人電子郵件填入表單<br/>確認後將發放超商點數卷<br/><br/><br/>按任意鍵關閉網頁" x=0 y=-32 z_index=0

define sequence EXP_gap_sc
	set flush_keyboard yes
	set description "Runs a number of items in sequence"
	run Prac_short_sc "[p_gap_sc]==0"
	run Prac_long_sc "[p_gap_sc]==1"
	run loop_setup_sc always
	run EXP_Instruction_sc always
	run EXP_short_sc "[e_gap_sc]==0"
	run EXP_long_sc "[e_gap_sc]==1"
	run EXP_complete_sc always

define sequence EXP_gap_tc
	set flush_keyboard yes
	set description "Runs a number of items in sequence"
	run Prac_short_tc "[p_gap_tc]==0"
	run Prac_long1_tc "[p_gap_tc]==1"
	run Prac_long2_tc "[p_gap_tc]==2"
	run EXP_Instruction_tc always
	run EXP_short_tc "[e_gap_tc]==0"
	run EXP_long1_tc "[e_gap_tc]==1"
	run EXP_long2_tc "[e_gap_tc]==2"
	run EXP_complete_tc never

