define notepad EXP_loop_setup
	__note__
	- 指導語(welcome_01, welcome_02)要提示如何進行反應。
	- 每次實驗只執行一個prac loop與一個EXP loop: short,long1,或long2
	- 每個loop的"target_shape”內square與diamond長寬要一致
	
	
	- 結束時要給「完成代碼」；生成的JS程式碼與最後呈現畫面參考教學網頁 -> "完成代碼產生器" https://scgeeker.github.io/Online_EXP/OS_script.html
	__end__
	set description "A simple notepad to document your experiment. This plug-in does nothing."

define loop EXP_sc
	set source table
	set repeat 0.5
	set order random
	set description "Repeatedly runs another item"
	set cycles 2
	set continuous no
	set break_if_on_first yes
	set break_if "[count_EXP_gap]=1"
	setcycle 0 p_gap_sc 0
	setcycle 0 e_gap_sc 0
	setcycle 1 p_gap_sc 1
	setcycle 1 e_gap_sc 1
	run EXP_gap_sc

define sequence EXP_seq_sc
	set flush_keyboard yes
	set description "Runs a number of items in sequence"
	run fixation_sc always
	run cue_sc always
	run interval_sc always
	run target_sc always
	run response_ans_sc always
	run decision_response_correct_sc "[correct]=1"
	run decision_response_wrong_sc "[correct]=0"
	run logger always

define notepad EXP_seq_setup
	__note__
	- EXP_sequence最後必須有logger
	__end__
	set description "A simple notepad to document your experiment. This plug-in does nothing."

