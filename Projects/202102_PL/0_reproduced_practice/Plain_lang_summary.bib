@report{benzPLanPsyInteressenkatalog2021,
  title = {PLan Psy - Interessenkatalog: Eine empirisch fundierte Klassifikation psychologiebezogener Interessen in der Allgemeinbevölkerung Entscheidungshilfe zur Selektion KLARtext-relevanter Meta-Analysen in PSYNDEX},
  shorttitle = {PLan Psy - Interessenkatalog},
  author = {Benz, Gesa and Jonas, Mark and Kerwer, Martin and Stoll, Marlene and Chasiotis, Anita},
  date = {2021-08-09},
  institution = {{PsychArchives}},
  doi = {10.23668/PSYCHARCHIVES.5034},
  url = {https://www.psycharchives.org/handle/20.500.12034/4460},
  urldate = {2021-10-18},
  abstract = {Das Projekt PLan Psy verfolgt das Ziel, evidenzbasierte Richtlinien für die Erstellung sogenannter KLARtexte psychologischer Meta-Analysen zu verfassen, um psychologische Forschungsergebnisse für die Allgemeinheit transparenter zu gestalten. KLAR steht dabei für kurz, laiengerecht und allgemeinverständlich sowie richtlinienkonform. Um sicherzustellen, dass diese KLARtexte das öffentliche Interesse an psychologischen Fragestellungen repräsentieren, wurden im Rahmen zweier Studien über 4.000 Personen befragt, zu welchen psychologischen Themen sie gerne mehr Informationen hätten. Die daraus resultierenden 20 Interessenkategorien werden mit diesem Katalog vorgestellt, welcher als Selektionskriterium für das Verfassen von KLARtexten für künftige KLARtext-Autorinnen und -Autoren dienen soll. Die Interessenkategorien werden im Katalog den zugehörigen Teilgebieten der Psychologie zugeordnet und mit einer in der Psychologie verwendeten Bezeichnung und Beschreibung versehen. Darüber hinaus finden sich im Interessenkatalog die entsprechenden allgemeinverständlichen Bezeichnungen und Beschreibungen sowie die jeweiligen inhaltlichen PSYNDEX-Klassifikationskategorien.},
  editora = {Leibniz Institut Für Psychologie (ZPID) and Leibniz Institut Für Psychologie (ZPID)},
  editoratype = {collaborator},
  langid = {german},
  keywords = {150,classification,general public interests,plain language summaries,psychological topics,PSYNDEX,science communication,user study}
}

